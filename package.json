{
  "name": "de-noun-parser",
  "version": "1.1.0",
  "description": "Parse Wikitext's de-noun template",
  "main": "dist/de-noun-parser.js",
  "types": "types/de-noun-parser.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json --force --fix '{src,test}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push --tags && rm -rf dist && rm -rf types"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maroun-baydoun/de-noun-parser.git"
  },
  "bugs": {
    "url": "https://github.com/maroun-baydoun/de-noun-parser/issues"
  },
  "homepage": "http://www.maroun-baydoun.com/projects/de-noun-parser/",
  "author": {
    "name": "Maroun Baydoun",
    "url": "http://www.maroun-baydoun.com/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "types"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.23",
    "jest": "24.9.0",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.7.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
