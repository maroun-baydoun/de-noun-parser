{
  "name": "de-noun-parser",
  "version": "2.2.0",
  "description": "Parse Wikitext's de-noun template",
  "main": "dist/de-noun-parser.js",
  "types": "types/de-noun-parser.d.ts",
  "scripts": {
    "prettier": "prettier --write './{src,test}/**/*.ts' './webpack.config.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maroun-baydoun/de-noun-parser.git"
  },
  "bugs": {
    "url": "https://github.com/maroun-baydoun/de-noun-parser/issues"
  },
  "author": {
    "name": "Maroun Baydoun",
    "url": "https://maroun-baydoun.com/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "types"
  ],
  "keywords": [
    "wiktionary",
    "parse",
    "dictionary",
    "german"
  ],
  "license": "MIT",
  "homepage": "https://dev.maroun-baydoun.com/de-noun-parser/",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "clean-webpack-plugin": "3.0.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.17",
    "typescript": "4.2.3",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
